func main ()
    Circle a (50, 50, 30);
    Fill a #0c0c80;
    Stroke a #008d80 2;
    ScaleRel a (2);
    Draw a;
    Loop Movement a 100. 100. 5.;

    Circle b (75, 90, 25);
    Stroke b #80d00c 5;
    Draw b;

    Write "We are the winners";
    Text t (50, 200, "We are the winners");
    Draw t;

endfunc
$SHOW
