    Loop a # "d" ("m0,100 L250,100 L500,100","m0,100 L250,400 L500,100","m0,100 L250,100 L500,100") {10};
    Draw a;

    Path b (0,0);
    Stroke b "#ffffff" 10;
    Loop b # "d" ("m0,250 L250,250 L500,250","m0,250 L250,250 L500,250","m0,250 L250,250 L500,250") {10};
    Draw b;

    Path c (0,0);
