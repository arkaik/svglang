    Loop c # "d" ("m0,400 L250,400 L500,400","m0,400 L250,100 L500,400","m0,400 L250,400 L500,400") {10};
    Draw c;

    Path d1 (0,0);
    Stroke d1 "#ffffff" 5;
    Loop d1 # "d" ("m250,0 L250,100 S250,250 250,400 L250,500","m250,0 L250,100 S500,250 250,400 L250,500","m250,0 L250,100 S250,250 250,400 L250,500") {10};
    Draw d1;

    Path d2 (0,0);
    Stroke d2 "#ffffff" 5;
    Loop d2 # "d" ("m250,0 L250,100 S250,250 250,400 L250,500","m250,0 L250,100 S0,250 250,400 L250,500","m250,0 L250,100 S250,250 250,400 L250,500") {10};
    Draw d2;

endfunc
